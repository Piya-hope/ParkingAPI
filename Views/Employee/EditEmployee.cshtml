                                                <div class="form-group">
                                                    @Html.TextBoxFor(model => model.Document, new { @class = "form-control", @placeholder = "Enter User PAN/ADHAR Number" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Status, htmlAttributes: new { @class = "control-label" })
                                                    <span style="color: #FF0000">*</span>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.RadioButtonFor(model => model.Status, "True")
                                                    @Html.Label("Active")
                                                    <br />
                                                    @Html.RadioButtonFor(model => model.Status, "False")
                                                    @Html.Label("InActive")
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6" style="font-size: 12px;">
                                            Note: <span style='color:#FF0000;font-size:12px'>*</span> indicate required fields.
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="form-group">
                                            <center>
                                                <input type="submit" value="Update" class="btn btn-primary btn-flat" TabIndex="13" id="Save" onclick="validateAndConfirm()" />&nbsp;&nbsp;&nbsp;&nbsp;
                                                <input type="button" value="Cancel" class="btn btn-danger btn-flat" TabIndex="14" onclick="location.href='@Url.Action("ViewEmployee", "Employee")'" />
                                            </center>
                                            <br />
                                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-offset-2 col-md-10" style="color:green">
                                                @if (ViewBag.Message != null)
                                                {
                                                    <text>
                                                        <script>
                                                        $(document).ready(function () {
                                                            alert('@ViewBag.Message');
                                                        });
                                                        </script>
                                                    </text>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        }
    </div>
</div>
        }

<script type="text/javascript">
    function Confirm() {
        var confirm_value = document.createElement("INPUT");
        confirm_value.type = "hidden";
        confirm_value.name = "confirm_value";
        if (confirm("Do you want to save data?")) {
            confirm_value.value = "Yes";
            document.forms[0].appendChild(confirm_value);
        } else {
            confirm_value.value = "No";
            document.forms[0].appendChild(confirm_value);
        }
    }
    function validateAndConfirm() {
        var confirm_value = document.createElement("INPUT");
        Page_ClientValidate("Save");  //'GroupName' is the ValidationGroup
        if (Page_IsValid) {
            confirm_value.type = "hidden";
            confirm_value.name = "confirm_value";
            if (confirm("Please confirm Update value, Before Click OK Button")) {
                confirm_value.value = "Yes";
                document.forms[0].appendChild(confirm_value);
            } else {
                confirm_value.value = "No";
                document.forms[0].appendChild(confirm_value);
            }
        }
    }
</script>
