                                        <div class="col-md-6">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.User_Name, htmlAttributes: new { @class = "control-label" })
                                                    <span style="color: #FF0000">*</span>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.TextBoxFor(model => model.User_Name, new { @class = "form-control", @placeholder = "Enter User Name" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Email_Id, htmlAttributes: new { @class = "control-label" })
                                                    <span style="color: #FF0000">*</span>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.TextBoxFor(model => model.Email_Id, new { @class = "form-control", @placeholder = "Enter User Email" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Mobile_No, htmlAttributes: new { @class = "control-label" })
                                                    <span style="color: #FF0000">*</span>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.TextBoxFor(model => model.Mobile_No, new { @class = "form-control", @placeholder = "Enter User Mobile", @maxlength = "10" })
                                                </div>
                                            </div>
                                        </div>
