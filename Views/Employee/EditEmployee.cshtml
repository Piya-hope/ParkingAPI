                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Document, htmlAttributes: new { @class = "control-label" })

                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.TextBoxFor(model => model.Document, new { @class = "form-control", @placeholder = "Enter User PAN/ADHAR Number" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
